<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="scheduler.wgu.mywguscheduler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyWGUScheduler">
        <activity android:name=".UI.InstructorDetailActivity"></activity>
        <activity android:name=".UI.TermDetailActivity" />
        <activity android:name=".UI.AddTermActivity" />
        <activity android:name=".UI.AddInstructorActivity" />
        <activity android:name=".UI.AddCourseActivity" />
        <activity android:name=".UI.AddAssessmentActivity" />
        <activity android:name=".UI.TermsActivity" />
        <activity android:name=".UI.InstructorActivity" />
        <activity android:name=".UI.CoursesActivity" />
        <activity android:name=".UI.AssessmentActivity" />
        <activity android:name=".UI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>